<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.meskius.attemp2.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    android:background="@drawable/bg12">

    <Button
        android:id="@+id/refresh"
        android:layout_width="32dp"
        android:layout_height="34dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="*"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Spinner
        android:id="@+id/grupeID"
        android:layout_width="56dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#42dff4"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097"
        app:layout_constraintHorizontal_bias="0.459" />

    <Spinner
        android:id="@+id/metaiID"
        android:layout_width="56dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:background="#42dff4"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Spinner
        android:id="@+id/tipasID"
        android:layout_width="56dp"
        android:layout_height="31dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="#42dff4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.035"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/program"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11dp"
        android:text="Tipas"
        app:layout_constraintBottom_toTopOf="@+id/tipasID"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintVertical_bias="0.45"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.047" />

    <TextView
        android:id="@+id/year"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Metai"
        app:layout_constraintBottom_toTopOf="@+id/metaiID"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.182"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.529" />

    <TextView
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginBottom="0dp"
        android:text="Grupe"
        app:layout_constraintBottom_toTopOf="@+id/grupeID"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/savaitinis"
        android:layout_width="146dp"
        android:layout_height="52dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:text="Rodyti savaitini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintHorizontal_bias="0.788"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/branchID"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/branchID"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="18dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#42dff4"
        android:text="Studiju sritis"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintRight_toLeftOf="@+id/grupeID"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.108" />

    <Button
        android:id="@+id/savAtgal"
        android:layout_width="83dp"
        android:layout_height="51dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="Savaite Atgal"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintHorizontal_bias="0.649"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/savPirmyn"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/branchID"
        app:layout_constraintVertical_bias="0.933" />

    <Button
        android:id="@+id/savPirmyn"
        android:layout_width="83dp"
        android:layout_height="51dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="16dp"
        android:text="Savaite pirmyn"
        app:layout_constraintBottom_toTopOf="@+id/webView"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/branchID"
        app:layout_constraintVertical_bias="0.933" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="0dp"
        android:layout_height="226dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.743"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="133dp"
        tools:layout_editor_absoluteX="-91dp" />

</android.support.constraint.ConstraintLayout>
